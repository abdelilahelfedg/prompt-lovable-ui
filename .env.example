# OpenHome Frontend Environment Variables

# App Configuration
VITE_APP_NAME=OpenHome
VITE_APP_URL=http://localhost:8080

# API Configuration (for future backend integration)
VITE_API_URL=http://localhost:3000/api
VITE_API_VERSION=v1

# Map Configuration (for real map integration)
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Authentication (for future auth integration)
VITE_AUTH_DOMAIN=your_auth_domain
VITE_AUTH_CLIENT_ID=your_auth_client_id

# Third-party Services
VITE_ANALYTICS_ID=your_analytics_id
VITE_SENTRY_DSN=your_sentry_dsn

# Feature Flags
VITE_ENABLE_REAL_MAP=false
VITE_ENABLE_PAYMENT=false
VITE_ENABLE_CHAT=false